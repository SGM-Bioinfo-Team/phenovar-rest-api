{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "urn:github:phenovar-rest-api/phenovar-response.schema.json",
  "title": "Phenovar REST API response for endpoint: dxtablegenerator/generate",
  "description": "JSON response for the Phenovar REST API endpoint dxtablegenerator/generate.",
  "type": "object",
  "properties": {
    "disease_gene_list": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "gene": {
            "type": "object",
            "properties": {
              "symbol": {
                "type": "string",
                "minLength": 1,
                "title": "Gene symbol",
                "description": "Gene symbol."
              },
              "id_hgnc": {
                "type": "string",
                "minLength": 1,
                "title": "Gene HGNC ID",
                "description": "Gene HGNC ID."
              },
              "id_ensembl": {
                "type": "string",
                "minLength": 1,
                "title": "Gene Ensembl ID",
                "description": "Gene Ensembl ID."
              },
              "id_ncbi": {
                "type": "string",
                "minLength": 1,
                "title": "Gene NCBI ID",
                "description": "Gene NCBI ID (Entrez ID)."
              }
            },
            "required": [
              "symbol",
              "id_hgnc",
              "id_ensembl",
              "id_ncbi"
            ],
            "title": "Gene",
            "description": "Gene."
          },
          "disease": {
            "type": "object",
            "properties": {
              "omim_code": {
                "type": "string",
                "minLength": 1,
                "title": "Disease OMIM code",
                "description": "Disease OMIM code."
              },
              "omim_name": {
                "type": "string",
                "title": "Disease OMIM name",
                "description": "Disease OMIM name."
              }
            },
            "required": [
              "omim_code",
              "omim_name"
            ],
            "title": "Disease",
            "description": "Disease. Can be None/null when filtering for GUS."
          },
          "inheritance_mode_list": {
            "type": "array",
            "items": {
              "type": "string",
              "minLength": 1
            },
            "title": "Inheritance mode list",
            "description": "Inheritance mode list."
          },
          "annotation_data": {
            "type": "object",
            "properties": {
              "is_clingen_hi": {
                "type": "boolean",
                "title": "Is Clingen HI",
                "description": "Is Clingen haploinsufficient."
              },
              "is_clingen_ts": {
                "type": "boolean",
                "title": "Is Clingen TS",
                "description": "Is Clingen triplosensitive."
              },
              "pli_score": {
                "type": [
                  "number",
                  "null"
                ],
                "title": "Haploinsufficiency pLi score",
                "description": "Haploinsufficiency pLi score."
              },
              "predicted_to_hi": {
                "type": "boolean",
                "title": "Predicted to HI",
                "description": "Predicted to haploinsufficiency by pLI score > 0.9."
              }
            },
            "required": [
              "is_clingen_hi",
              "is_clingen_ts",
              "pli_score",
              "predicted_to_hi"
            ],
            "title": "Disease gene annotation data",
            "description": "Selected annotation data for this disease gene."
          },
          "patient_phenotype_match_list": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "patient_hpo_code": {
                  "type": "string",
                  "minLength": 1,
                  "title": "Patient phenotype HPO term code",
                  "description": "Patient phenotype HPO term code."
                },
                "patient_hpo_name": {
                  "type": "string",
                  "minLength": 1,
                  "title": "Patient phenotype HPO term name",
                  "description": "Patient phenotype HPO term name."
                },
                "is_matching": {
                  "type": "boolean",
                  "title": "Patient phenotype is matching disease",
                  "description": "Patient phenotype is matching disease phenotype."
                },
                "matching_level": {
                  "type": [
                    "integer",
                    "null"
                  ],
                  "title": "Patient phenotype matching level",
                  "description": "Patient phenotype matching level vs disease phenotype. Can be None/null.\n        <ul>\n            <li>0: exact match, e.g. both patient and disease term are exactly the same.</li>\n            <li>>0: patient term is more specific by this amount of hierarchy level than disease term.</li>\n                <ul><li>E.g.: value=3 meaning that patient term \"Ventricular septal defect\" is more specific by three level than disease term \"Abnormal heart morphology\".</li></ul>\n            <li><0: patient term is more general by this amount of hierarchy level than disease term.</li>\n                <ul><li>E.g.: value=-1 meaning that patient term \"Growth delay\" is more general by one level than disease term \"Short stature\".</li></ul>\n            <li>None/null: no match.</li>\n        </ul>\n        "
                },
                "disease_hpo_code": {
                  "type": [
                    "string",
                    "null"
                  ],
                  "minLength": 1,
                  "title": "Disease phenotype HPO term code",
                  "description": "Disease phenotype HPO term code. None/null if no match."
                },
                "disease_hpo_name": {
                  "type": [
                    "string",
                    "null"
                  ],
                  "minLength": 1,
                  "title": "Disease phenotype HPO term name",
                  "description": "Disease phenotype HPO term name. None/null if no match."
                }
              },
              "required": [
                "patient_hpo_code",
                "patient_hpo_name",
                "is_matching",
                "disease_hpo_code",
                "disease_hpo_name"
              ]
            },
            "title": "Patient phenotype match data list",
            "description": "List of phenotype matching data between patient and disease."
          },
          "disease_onset_match_list": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "patient_hpo_code": {
                  "type": "string",
                  "minLength": 1,
                  "title": "Patient phenotype HPO term code",
                  "description": "Patient phenotype HPO term code."
                },
                "patient_hpo_name": {
                  "type": "string",
                  "minLength": 1,
                  "title": "Patient phenotype HPO term name",
                  "description": "Patient phenotype HPO term name."
                },
                "is_matching": {
                  "type": "boolean",
                  "title": "Patient phenotype is matching disease",
                  "description": "Patient phenotype is matching disease phenotype."
                },
                "matching_level": {
                  "type": [
                    "integer",
                    "null"
                  ],
                  "title": "Patient phenotype matching level",
                  "description": "Patient phenotype matching level vs disease phenotype. Can be None/null.\n        <ul>\n            <li>0: exact match, e.g. both patient and disease term are exactly the same.</li>\n            <li>>0: patient term is more specific by this amount of hierarchy level than disease term.</li>\n                <ul><li>E.g.: value=3 meaning that patient term \"Ventricular septal defect\" is more specific by three level than disease term \"Abnormal heart morphology\".</li></ul>\n            <li><0: patient term is more general by this amount of hierarchy level than disease term.</li>\n                <ul><li>E.g.: value=-1 meaning that patient term \"Growth delay\" is more general by one level than disease term \"Short stature\".</li></ul>\n            <li>None/null: no match.</li>\n        </ul>\n        "
                },
                "disease_hpo_code": {
                  "type": [
                    "string",
                    "null"
                  ],
                  "minLength": 1,
                  "title": "Disease phenotype HPO term code",
                  "description": "Disease phenotype HPO term code. None/null if no match."
                },
                "disease_hpo_name": {
                  "type": [
                    "string",
                    "null"
                  ],
                  "minLength": 1,
                  "title": "Disease phenotype HPO term name",
                  "description": "Disease phenotype HPO term name. None/null if no match."
                }
              },
              "required": [
                "patient_hpo_code",
                "patient_hpo_name",
                "is_matching",
                "disease_hpo_code",
                "disease_hpo_name"
              ]
            },
            "title": "Disease onset match data list",
            "description": "List of onset matching data between disease and patient. Absent (None/null) if patient have no onset data."
          },
          "diagnosis_likelihood": {
            "type": "object",
            "properties": {
              "label": {
                "type": "string",
                "enum": [
                  "very likely",
                  "likely",
                  "possible",
                  "unlikely"
                ],
                "title": "Diagnosis likelihood label",
                "description": "Diagnosis likelihood label."
              },
              "rank": {
                "type": "integer",
                "title": "Diagnosis likelihood rank",
                "description": "Diagnosis likelihood rank."
              },
              "score": {
                "type": "number",
                "title": "Diagnosis likelihood score",
                "description": "Diagnosis likelihood score."
              },
              "filter_mode": {
                "type": "string",
                "enum": [
                  "phenotype_specific",
                  "incidental_findings",
                  "gus"
                ],
                "title": "Diagnosis likelihood filter mode",
                "description": "Diagnosis likelihood filter mode."
              }
            },
            "required": [
              "label",
              "rank",
              "score",
              "filter_mode"
            ],
            "title": "Diagnosis likelihood score data",
            "description": "Diagnosis likelihood score data."
          },
          "diagnosis_likelihood_history": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "text": {
                  "type": "string",
                  "minLength": 1,
                  "title": "Score cause text",
                  "description": "Text describing a cause for the score value."
                },
                "score": {
                  "type": "number",
                  "title": "Score cause value",
                  "description": "Value for the score cause."
                },
                "label": {
                  "type": "string",
                  "minLength": 1,
                  "title": "Score cause label",
                  "description": "Label for the score cause."
                }
              },
              "required": [
                "text",
                "score"
              ]
            },
            "title": "Diagnosis likelihood scoring history",
            "description": "Diagnosis likelihood scoring history."
          },
          "snv_list": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "signature": {
                  "type": "string",
                  "minLength": 1,
                  "title": "Variant signature",
                  "description": "Variant signature."
                },
                "signature_separator": {
                  "type": "string",
                  "minLength": 1,
                  "title": "Variant signature separator",
                  "description": "Variant signature separator."
                },
                "causality": {
                  "type": "string",
                  "enum": [
                    "causal",
                    "supplementary"
                  ],
                  "title": "Variant causality",
                  "description": "Variant causality: likely_causative, likely_non_causative, non_causative."
                },
                "score": {
                  "type": "object",
                  "properties": {
                    "label": {
                      "type": "string",
                      "enum": [
                        "pathogenic",
                        "likely pathogenic",
                        "uncertain significance high",
                        "uncertain significance",
                        "uncertain significance low",
                        "likely benign",
                        "benign"
                      ],
                      "title": "Variant score label",
                      "description": "Variant score label."
                    },
                    "value": {
                      "type": "number",
                      "title": "Variant score value",
                      "description": "Variant score value."
                    }
                  },
                  "required": [
                    "label",
                    "value"
                  ],
                  "title": "Variant score",
                  "description": "Variant score."
                },
                "score_history": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "text": {
                        "type": "string",
                        "minLength": 1,
                        "title": "Score cause text",
                        "description": "Text describing a cause for the score value."
                      },
                      "score": {
                        "type": "number",
                        "title": "Score cause value",
                        "description": "Value for the score cause."
                      },
                      "label": {
                        "type": "string",
                        "minLength": 1,
                        "title": "Score cause label",
                        "description": "Label for the score cause."
                      }
                    },
                    "required": [
                      "text",
                      "score"
                    ]
                  },
                  "title": "Variant scoring history",
                  "description": "Variant scoring history."
                },
                "description": {
                  "type": "object",
                  "properties": {
                    "transcript": {
                      "type": "string",
                      "minLength": 1,
                      "title": "SNV transcript",
                      "description": "SNV transcript."
                    },
                    "effect_name": {
                      "type": "string",
                      "minLength": 1,
                      "title": "SNV effect name",
                      "description": "SNV effect name."
                    },
                    "effect_hgvs": {
                      "type": "string",
                      "minLength": 1,
                      "title": "SNV effect HGVS",
                      "description": "SNV effect HGVS nomenclature."
                    }
                  },
                  "required": [
                    "transcript",
                    "effect_name",
                    "effect_hgvs"
                  ],
                  "title": "Variant description",
                  "description": "Variant description."
                },
                "annotation": {
                  "type": "object",
                  "properties": {
                    "parental_origin": {
                      "type": [
                        "string",
                        "null"
                      ],
                      "minLength": 1,
                      "title": "SNV parental origin",
                      "description": "SNV parental origin. None/null when not applicable."
                    },
                    "parental_origin_zigosity": {
                      "type": [
                        "string",
                        "null"
                      ],
                      "minLength": 1,
                      "title": "SNV parental origin zigosity",
                      "description": "SNV parental origin zigosity. None/null when not applicable."
                    }
                  },
                  "required": [
                    "parental_origin",
                    "parental_origin_zigosity"
                  ],
                  "title": "Variant annotation",
                  "description": "Variant annotation."
                }
              },
              "required": [
                "signature",
                "causality",
                "score",
                "score_history",
                "description",
                "annotation"
              ]
            },
            "title": "SNV data list",
            "description": "List of SNV data contributing to the diagnosis likelihood score."
          },
          "cnv_list": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "signature": {
                  "type": "string",
                  "minLength": 1,
                  "title": "Variant signature",
                  "description": "Variant signature."
                },
                "signature_separator": {
                  "type": "string",
                  "minLength": 1,
                  "title": "Variant signature separator",
                  "description": "Variant signature separator."
                },
                "causality": {
                  "type": "string",
                  "enum": [
                    "causal",
                    "supplementary"
                  ],
                  "title": "Variant causality",
                  "description": "Variant causality: likely_causative, likely_non_causative, non_causative."
                },
                "score": {
                  "type": "object",
                  "properties": {
                    "label": {
                      "type": "string",
                      "enum": [
                        "pathogenic",
                        "likely pathogenic",
                        "uncertain significance high",
                        "uncertain significance",
                        "uncertain significance low",
                        "likely benign",
                        "benign"
                      ],
                      "title": "Variant score label",
                      "description": "Variant score label."
                    },
                    "value": {
                      "type": "number",
                      "title": "Variant score value",
                      "description": "Variant score value."
                    }
                  },
                  "required": [
                    "label",
                    "value"
                  ],
                  "title": "Variant score",
                  "description": "Variant score."
                },
                "score_history": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "text": {
                        "type": "string",
                        "minLength": 1,
                        "title": "Score cause text",
                        "description": "Text describing a cause for the score value."
                      },
                      "score": {
                        "type": "number",
                        "title": "Score cause value",
                        "description": "Value for the score cause."
                      },
                      "label": {
                        "type": "string",
                        "minLength": 1,
                        "title": "Score cause label",
                        "description": "Label for the score cause."
                      }
                    },
                    "required": [
                      "text",
                      "score"
                    ]
                  },
                  "title": "Variant scoring history",
                  "description": "Variant scoring history."
                },
                "description": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string",
                      "enum": [
                        "DEL",
                        "DUP"
                      ],
                      "title": "CNV type",
                      "description": "CNV type."
                    },
                    "transcript": {
                      "type": [
                        "string",
                        "null"
                      ],
                      "minLength": 1,
                      "title": "CNV transcript",
                      "description": "CNV transcript. None/null if intergenic."
                    },
                    "exon_range": {
                      "type": [
                        "string",
                        "null"
                      ],
                      "minLength": 1,
                      "title": "CNV exon range",
                      "description": "CNV exon range. None/null if intronic/intergenic."
                    },
                    "is_intronic": {
                      "type": "boolean",
                      "title": "CNV is intronic",
                      "description": "CNV is intronic."
                    },
                    "is_intergenic": {
                      "type": "boolean",
                      "title": "CNV is intergenic",
                      "description": "CNV is intergenic."
                    }
                  },
                  "required": [
                    "type",
                    "transcript",
                    "exon_range",
                    "is_intronic",
                    "is_intergenic"
                  ],
                  "title": "Variant description",
                  "description": "Variant description."
                },
                "annotation": {
                  "type": "object",
                  "properties": {
                    "dragen_filter": {
                      "type": "string",
                      "minLength": 1,
                      "title": "Dragen Filter",
                      "description": "Dragen VCF FILTER column value."
                    },
                    "dragen_copynb": {
                      "type": "integer",
                      "title": "Dragen copy number",
                      "description": "Dragen copy number value."
                    },
                    "dragen_meanratio": {
                      "type": "number",
                      "title": "Dragen mean ratio",
                      "description": "Dragen mean ratio value."
                    },
                    "dragen_phscore": {
                      "type": "number",
                      "title": "Dragen phred score",
                      "description": "Dragen phred score value."
                    },
                    "clingen_cnv_data": {
                      "type": [
                        "string",
                        "null"
                      ],
                      "minLength": 1,
                      "title": "Clingen CNV Data",
                      "description": "Clingen CNV Data, if overlapping with a Clingen CNV. None/null otherwise."
                    },
                    "dgv_data": {
                      "type": [
                        "boolean",
                        "null"
                      ],
                      "title": "DGV Data",
                      "description": "Data from overlapping entries in the Database of Genomic Variants: https://dgv.tcag.ca. None/null if not overlapping."
                    },
                    "parental_origin": {
                      "type": [
                        "string",
                        "null"
                      ],
                      "minLength": 1,
                      "title": "CNV parental origin",
                      "description": "CNV parental origin. None/null when not applicable."
                    }
                  },
                  "required": [
                    "dragen_filter",
                    "dragen_copynb",
                    "dragen_meanratio",
                    "dragen_phscore",
                    "clingen_cnv_data",
                    "dgv_data",
                    "parental_origin"
                  ],
                  "title": "Variant annotation",
                  "description": "Variant annotation."
                }
              },
              "required": [
                "signature",
                "causality",
                "score",
                "score_history",
                "description",
                "annotation"
              ]
            },
            "title": "CNV data list",
            "description": "List of CNV data contributing to the diagnosis likelihood score."
          }
        },
        "required": [
          "gene",
          "disease",
          "inheritance_mode_list",
          "annotation_data",
          "patient_phenotype_match_list",
          "disease_onset_match_list",
          "diagnosis_likelihood",
          "diagnosis_likelihood_history",
          "snv_list",
          "cnv_list"
        ]
      },
      "title": "Disease gene list",
      "description": "List of annotations and diagnosis likelihood scoring data for a disease gene."
    }
  },
  "required": [
    "disease_gene_list"
  ]
}
